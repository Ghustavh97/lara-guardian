{
    "name": "ghustavh97/larakey",
    "description": "Permission and role handling for Laravel 5.8 and up inspired by laravel-permissions by Spatie",
    "keywords": [
        "spatie",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security",
        "ghustavh97",
        "authority",
        "larakey"
    ],
    "homepage": "https://github.com/Ghustavh97/larakey/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ghustavh Ehm",
            "email": "ghustavh97@gmail.com",
            "homepage": "https://github.com/Ghustavh97",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/auth": "^5.8|^6.0|^7.0",
        "illuminate/container": "^5.8|^6.0|^7.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Ghustavh97\\Larakey\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ghustavh97\\Larakey\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ghustavh97\\Larakey\\LarakeyServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
